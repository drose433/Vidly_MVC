@model Vidly.Models.Customer

@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
    <ul>
        @if (!Model.Birthday.IsEmpty())
        { 
            <li>Birthday: @Model.Birthday</li>
        }
    
            <li>MembershipType: @Model.MembershipType.Name</li>
    

    </ul>